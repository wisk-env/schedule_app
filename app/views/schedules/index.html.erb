<h1>予定一覧</h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>タイトル</th>
      <th>開始日</th>
      <th>終了日</th>
      <th>終日</th>
      <th>更新日時</th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.id %></td>
        <td><%= schedule.title %></td>
        <td><%= schedule.start_date %></td>
        <td><%= schedule.end_date %></td>
        <td><%= schedule.all_day %></td>
        <td><%= schedule.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p><%= link_to "新規追加", new_schedule_path %></p>
